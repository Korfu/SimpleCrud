@using SimpleCrud.Entities
@model User

<div class=" form-group">
    <label for="FristName">Imię</label>
    @Html.EditorFor(m=>m.FirstName, new { @class = "form-control" })
</div>
<div class=" form-group">
    <label for="LastName">Naziwsko</label>
    @Html.EditorFor(m => m.LastName, new { @class = "form-control" })
</div>
<div class=" form-group">
    <label for="DateOfBirth">Data Urodzenia</label>
    @Html.EditorFor(m => m.DateOfBirth, new { @class = "form-control" })
</div>

<input type="submit" class="btn btn-primary" value="Zapisz"/>
